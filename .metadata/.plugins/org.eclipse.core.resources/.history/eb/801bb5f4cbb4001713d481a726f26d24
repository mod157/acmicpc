package step3;
/*
 * 11단계 No.11721 열 개씩 끊어 출력하기
 */
import java.util.Scanner;

public class No_11 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		int sum = 0;
		for(int i = 1; i <= a; i++){
			sum += b % 10;
			b /= 10;
		}
		System.out.print(sum);
	}
}
