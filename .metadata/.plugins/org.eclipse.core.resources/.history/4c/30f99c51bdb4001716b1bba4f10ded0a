package step2;
/*
 * 8단계 2839번  설탕배달
 */

import java.util.Scanner;
public class No_8 {
	public static void main(String args[]){
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		
		if(a % 5 == 0){
			System.out.print(a/5);
		}else if(a % 5 == 3){
			System.out.print(a/5 + (a%5)/3);
		}
		
		
	}
}
