package step3;
/*
 * 10단계 No.11720 숫자의 합
 */
import java.util.Scanner;

public class No_10 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		int sum = 0;
		for(int i = 1; i <= a; i++){
			sum += b % 10;
			b /= 10;
		}
		System.out.print(sum);
	}

}
