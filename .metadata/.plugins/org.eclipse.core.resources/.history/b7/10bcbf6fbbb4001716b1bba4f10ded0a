package step2;
/*
 * 6단계 10430번  나머지
 */

import java.util.Scanner;
public class No_6 {
	public static void main(String args[]){
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.print(a%b);;
	}
}
