package step4;
/*
 * 2단계 No.10817 세 수
 */

import java.util.Scanner;

public class No_2 {
	public static void main(String args[]){
		Scanner scan = new Scanner(System.in);
		int a = scan.nextInt();
		int b = scan.nextInt();
		int c = scan.nextInt();
		
		if( a >= b){
			//a가 더 큼
			if(b >= c){
				System.out.print(b);
			}else{
				System.out.print(a);
			}
		}else{
			//b가 작음
			if(b >= c){
				System.out.print(c);
			}else{
				System.out.print(b);
			}
		}
	}
}
